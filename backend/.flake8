[flake8]
ignore = C101,D403,F403,F405,I100,I201,W503,W504
exclude =
    **/migrations/*,
    .git,
    __pycache__,
    .venv
    frontend
filename = *.py
accept-encodings = utf-8
max-complexity = 12
max-linenumber = 500
max-line-length = 119
inline-quotes = double
multiline-quotes = double
indent-style = tab
